				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.25rem"
					},
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "1.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2rem"
					},
					"name": "2XL",
					"size": "2rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "2.5rem"
					},
					"name": "3XL",
					"size": "2.5rem",
					"slug": "3-x-large"
				},
				{
					"fluid": {
						"max": "6rem",
						"min": "3rem"
					},
					"name": "Display",
					"size": "3rem",
					"slug": "display"
				},
				{
					"fluid": {
						"max": "8rem",
						"min": "4rem"
					},
					"name": "Display Large",
					"size": "4rem",
					"slug": "display-large"
				}
			],
			"fontFamilies": [
				{
					"name": "Space Grotesk",
					"slug": "space-grotesk",
					"fontFamily": "'Space Grotesk', sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/space-grotesk/SpaceGrotesk-VariableFont_wght.woff2"
							],
							"fontWeight": "300 700",
							"fontStyle": "normal",
							"fontFamily": "Space Grotesk"
						}
					]
				},
				{
					"name": "Outfit",
					"slug": "outfit",
					"fontFamily": "Outfit, sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/outfit/Outfit-VariableFont_wght.woff2"
							],
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontFamily": "Outfit"
						}
					]
				},
				{
					"name": "Manrope",
					"slug": "manrope",
					"fontFamily": "Manrope, sans-serif",
					"fontFace": [
						{
							"src": [
