			"core/query-pagination": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "500"
				}
			},
			"core/search": {
				"css": "& .wp-block-search__input { border: 2px solid var(--wp--preset--color--contrast); border-radius: 0; padding: 0.75rem 1rem; } & .wp-block-search__input:focus { outline: 2px solid var(--wp--preset--color--accent-1); outline-offset: 2px; }",
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"lineHeight": "1.6"
				},
				"elements": {
					"button": {
						"border": {
							"radius": "0"
						},
						"spacing": {
							"margin": {
								"left": "0.5rem"
							}
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"color": "var:preset|color|contrast",
					"style": "solid",
					"width": "0 0 4px 0"
				},
				"color": {
					"text": "var:preset|color|contrast"
				},
				"variations": {
					"wide": {
						"css": " &:not(.alignfull){max-width: var(--wp--style--global--wide-size) !important;}"
					}
				}
			},
			"core/site-tagline": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"core/site-title": {
				"typography": {
					"fontWeight": "700",
					"letterSpacing": "-.5px"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/term-description": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var:preset|font-family|space-grotesk",
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "700",
					"letterSpacing": "0.05em",
					"textTransform": "uppercase"
				},
				"css": "& .wp-block-navigation__responsive-container-open { border: 4px solid #000; background: var(--neo-secondary); box-shadow: var(--neo-shadow-sm); padding: 0.5rem; } & .wp-block-navigation__responsive-container-open:hover { box-shadow: var(--neo-shadow-md); transform: translate(-2px, -2px); } & .wp-block-navigation__responsive-container-open:active { box-shadow: none; transform: translate(2px, 2px); } & .wp-block-navigation__responsive-container-open svg { width: 24px; height: 24px; stroke-width: 3; } & .wp-block-navigation__responsive-container-close { border: 4px solid #000; background: var(--neo-accent); box-shadow: var(--neo-shadow-sm); } & .wp-block-navigation-item__content { padding: 0.5rem 1rem; transition: all 0.1s ease-out; } & .wp-block-navigation-item__content:hover { background: var(--neo-accent); border: 4px solid #000; box-shadow: var(--neo-shadow-sm); margin: -4px; } & .wp-block-navigation__submenu-container { background: var(--neo-cream); border: 4px solid #000; box-shadow: var(--neo-shadow-md); margin-top: 8px !important; padding: 0.5rem; } & .wp-block-navigation__submenu-container .wp-block-navigation-item__content { padding: 0.75rem 1rem; } & .wp-block-navigation__submenu-container .wp-block-navigation-item__content:hover { background: var(--neo-secondary); }",
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|contrast"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|contrast"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"outline": {
								"color": "var:preset|color|contrast",
								"offset": "2px",
								"style": "solid",
								"width": "2px"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/list": {
				"css": "& li{margin-top: 0.5rem;}"
			}
		},
		"elements": {
			"button": {
				"border": {
					"color": "var:preset|color|contrast",
					"style": "solid",
					"width": "2px",
					"radius": "0"
				},
				"color": {
					"background": "var:preset|color|contrast",
					"text": "var:preset|color|accent-5"
				},
				"shadow": "var(--bauhaus-shadow-sm)",
				":focus": {
					"outline": {
						"color": "var:preset|color|accent-1",
						"offset": "2px",
						"style": "solid",
						"width": "2px"
					}
				},
				":hover": {
					"color": {
						"background": "color-mix(in srgb, var(--wp--preset--color--contrast) 90%, transparent)",
						"text": "var:preset|color|accent-5"
					}
				},
